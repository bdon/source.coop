{
  "dataset_info": {
    "name": "EuroSAT Land Cover Classification",
    "version": "2.0.0",
    "description": "Sentinel-2 satellite imagery covering European land cover types",
    "citation": "Helber, P., Bischke, B., Dengel, A., & Borth, D. (2019). EuroSAT: A Novel Dataset and Deep Learning Benchmark for Land Use and Land Cover Classification. IEEE Journal of Selected Topics in Applied Earth Observations and Remote Sensing.",
    "license": "CC BY-NC-SA 4.0",
    "homepage": "https://github.com/phelber/eurosat"
  },
  "splits": {
    "train": {
      "num_examples": 21000,
      "file_pattern": "train-*.zarr",
      "class_distribution": {
        "AnnualCrop": 2500,
        "Forest": 3000,
        "HerbaceousVegetation": 2000,
        "Highway": 2000,
        "Industrial": 2500,
        "Pasture": 2000,
        "PermanentCrop": 2000,
        "Residential": 2500,
        "River": 1500,
        "SeaLake": 1000
      }
    },
    "validation": {
      "num_examples": 4500,
      "file_pattern": "val-*.zarr",
      "class_distribution": {
        "AnnualCrop": 500,
        "Forest": 600,
        "HerbaceousVegetation": 400,
        "Highway": 400,
        "Industrial": 500,
        "Pasture": 400,
        "PermanentCrop": 400,
        "Residential": 500,
        "River": 400,
        "SeaLake": 400
      }
    },
    "test": {
      "num_examples": 4500,
      "file_pattern": "test-*.zarr",
      "class_distribution": {
        "AnnualCrop": 500,
        "Forest": 600,
        "HerbaceousVegetation": 400,
        "Highway": 400,
        "Industrial": 500,
        "Pasture": 400,
        "PermanentCrop": 400,
        "Residential": 500,
        "River": 400,
        "SeaLake": 400
      }
    }
  },
  "classes": {
    "AnnualCrop": {
      "id": 0,
      "description": "Annual crops like corn, wheat, sunflower, etc.",
      "color": "#FFD700"
    },
    "Forest": {
      "id": 1,
      "description": "Dense forest areas with high tree coverage",
      "color": "#228B22"
    },
    "HerbaceousVegetation": {
      "id": 2,
      "description": "Areas with natural herbaceous vegetation",
      "color": "#90EE90"
    },
    "Highway": {
      "id": 3,
      "description": "Major roads and transportation infrastructure",
      "color": "#808080"
    },
    "Industrial": {
      "id": 4,
      "description": "Industrial and commercial areas",
      "color": "#CD5C5C"
    },
    "Pasture": {
      "id": 5,
      "description": "Grasslands used for grazing",
      "color": "#8FBC8F"
    },
    "PermanentCrop": {
      "id": 6,
      "description": "Permanent crops like vineyards and olive groves",
      "color": "#DAA520"
    },
    "Residential": {
      "id": 7,
      "description": "Urban residential areas",
      "color": "#FA8072"
    },
    "River": {
      "id": 8,
      "description": "Rivers and water channels",
      "color": "#4169E1"
    },
    "SeaLake": {
      "id": 9,
      "description": "Seas, lakes and large water bodies",
      "color": "#000080"
    }
  },
  "features": {
    "image": {
      "shape": [64, 64, 13],
      "dtype": "float32",
      "bands": [
        "B01", "B02", "B03", "B04", "B05", "B06", 
        "B07", "B08", "B8A", "B09", "B10", "B11", "B12"
      ],
      "statistics": {
        "mean": [1340.64, 1096.79, 1050.33, 945.33, 1169.51, 2048.99, 
                2226.49, 2142.95, 2338.88, 722.73, 12.89, 1858.29, 1285.06],
        "std": [65.48, 153.68, 185.81, 278.83, 228.98, 355.59, 
               423.22, 437.20, 467.75, 124.47, 2.69, 357.08, 350.54]
      }
    },
    "label": {
      "num_classes": 10,
      "names": [
        "AnnualCrop", "Forest", "HerbaceousVegetation", "Highway",
        "Industrial", "Pasture", "PermanentCrop", "Residential",
        "River", "SeaLake"
      ]
    }
  },
  "preprocessing": {
    "normalization": "per_band_standardization",
    "augmentation": {
      "random_flip": true,
      "random_rotation": [-10, 10],
      "random_brightness": 0.2,
      "random_contrast": 0.2
    }
  },
  "metadata": {
    "spatial_coverage": {
      "min_lat": 35.0,
      "max_lat": 71.0,
      "min_lon": -25.0,
      "max_lon": 45.0
    },
    "temporal_coverage": {
      "start_date": "2023-01-01",
      "end_date": "2023-12-31"
    },
    "satellite_platform": "Sentinel-2",
    "processing_level": "L2A",
    "cloud_coverage_max": 10,
    "spatial_resolution": "10m",
    "coordinate_system": "EPSG:4326",
    "data_format": "Zarr",
    "total_size_gb": 89,
    "creation_date": "2024-01-15",
    "last_updated": "2024-03-01",
    "contact": {
      "name": "Radiant Earth Foundation",
      "email": "ml@radiant.earth",
      "website": "https://radiant.earth"
    }
  }
} 